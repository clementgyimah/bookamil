{
    "extends": [
      "tslint:recommended",
      "tslint-react",
      "tslint-config-prettier"
    ],
    "rulesDirectory": [
      "tslint-plugin-prettier"
    ],
    "rules": {
      "no-console": false,
      "prettier": true,
      "interface-name": false,
      "max-line-length": [
        true,
        {
          "limit": 120
        }
      ]
    }
  }